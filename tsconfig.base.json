{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"rootDir": "src",
		"removeComments": true,
		"noImplicitReturns": true,
		"strictNullChecks": true,
		"allowJs": false,
		"noUnusedParameters": true,
		"jsx": "react",
		"sourceMap": true,
		"noUnusedLocals": true,
		"moduleResolution": "node"
	}
}
